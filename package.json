{
  "name": "@gradecam/clever-api",
  "version": "2.0.2",
  "description": "Module for interacting with the Clever APIs.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && tsc -p .",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "Clever"
  ],
  "author": "GradeCam LLC",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^10.0.0",
    "@types/qs": "^6.5.3",
    "jest": "^24.9.0",
    "jest-junit": "^6.4.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "qs": "^6.8.0",
    "tslib": "^1.10.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(test.*)(^.?|\\.[^d]|[^.]d|[^.][^d])\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gradecam/clever-api.git"
  },
  "bugs": {
    "url": "https://github.com/gradecam/clever-api/issues"
  }
}
